<div class="main-container-shared-rental-view">
  <h2>Shared Rental for <%= current_user.name %></h2>
  <div id="map" style="width: 500px; height: 400px;"></div>
  <h3>People to share with</h3>
    <% @search_match_ids.each do |
  <h4>Description</h4>
  <h4><%= @my_search.description %></h4>
  <br>
  <h4>Time slots when the user will use the space</h4>
  <% Search::WEEK_DAYS.each_with_index do |week_day, index| %>
    <h5><%= "#{week_day} --> #{@my_search_times[index]}" %></h5>
  <% end %>
</div>

<% content_for :javascript_includes do %>
  <%= javascript_include_tag "show-map.js" %>
<% end %>

<% content_for :view_specific_css do %>
    <%= stylesheet_link_tag "_home-footer" %>
    <%= stylesheet_link_tag "_home-header" %>
    <%= stylesheet_link_tag "_shared-rental-view" %>
<% end %>

<script src="https://maps.google.com/maps/api/js" type="text/javascript"></script>