<div class="main-container-shared-rental-view">
  <h2>Shared Rental for <%= current_user.name %></h2>
  <div id="map" style="width: 500px; height: 400px;"></div>
  <h3>People to share with</h3>
    <% @print_info.each do |info| %>
      <h5><%= info[:person].get_full_name %></h5>
      <%= image_tag info[:person].avatar.url(:thumb) %>
      <h5><%= info[:desired_times] %></h5> 
    <% end %>
</div>

<% content_for :javascript_includes do %>
  <%= javascript_include_tag "show-google-map.js" %>
<% end %>

<% content_for :view_specific_css do %>
    <%= stylesheet_link_tag "_home-footer" %>
    <%= stylesheet_link_tag "_home-header" %>
    <%= stylesheet_link_tag "_shared-rental-view" %>
<% end %>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAv7a1XrJSqleGAzjwBh5OBLIzJ7co0L_0"
  type="text/javascript"></script>