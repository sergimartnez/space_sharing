<div class="main-container-space-new">
  <h1>Hi, <%= current_user.name %>!</h1>
  <h1>Do you want to rent a space?</h1>
  <h1>Let's do it in 3 simple steps!</h1>
  
  <%= render "space-form" %>

</div>

<% content_for :javascript_includes do %>
  <%= javascript_include_tag "new-space.js" %>
  <%= javascript_include_tag "slider.js" %>
<% end %>

<% content_for :view_specific_css do %>  
  <%= stylesheet_link_tag "_header" %>
  <%= stylesheet_link_tag "_space-new-main-container" %>
  <%= stylesheet_link_tag "_home-footer" %> 
<% end %>

