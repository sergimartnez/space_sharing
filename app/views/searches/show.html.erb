<div class="main-container-search-show">
  <div id="map" style="width: 500px; height: 400px;"></div>
  <h3><%= @my_search.title %></h3>
  <h4>Type: <%= @my_search.space_type %></h4>
  <h4>Description</h4>
  <h4><%= @my_search.description %></h4>
  <br>
  <h4>Availability</h4>
  <% Search::WEEK_DAYS.each_with_index do |week_day, index| %>
    <h5><%= "#{week_day} --> #{@my_search_times[index]}" %></h5>
  <% end %>
</div>

<% content_for :javascript_includes do %>
  <%= javascript_include_tag "show-map.js" %>
<% end %>

<script src="https://maps.google.com/maps/api/js" type="text/javascript"></script>